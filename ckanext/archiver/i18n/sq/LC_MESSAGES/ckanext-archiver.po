#, python-format
msgid "Invalid HTTP response: %s"
msgstr "Përgjigje HTTP e pavlefshme: %s"

#: ckanext/archiver/tasks.py:817 ckanext/archiver/tasks.py:964
#, python-format
msgid "Connection timed out after %ss"
msgstr "Lidhja përfundoi pas %ss"

