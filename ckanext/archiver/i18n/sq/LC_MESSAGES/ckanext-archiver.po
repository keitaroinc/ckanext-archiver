#, python-format
msgid "Error downloading: %s"
msgstr "Gabim gjatë shkarkimit: %s"

#: ckanext/archiver/tasks.py:825
#, python-format
msgid "Error with the download: %s"
msgstr "Gabim me shkarkimin: %s"

#: ckanext/archiver/tasks.py:955
msgid "Invalid URL or Redirect Link"
msgstr "URL e pavlefshme ose lidhje përcjellëse"

#: ckanext/archiver/tasks.py:958
msgid "Could not make HEAD request"
msgstr "Nuk mund të kërkonte HEAD"

#: ckanext/archiver/tasks.py:968
#, python-format
msgid "Error during request: %s"
msgstr "Gabim gjatë kërkesës: %s"

#: ckanext/archiver/tasks.py:970
#, python-format
msgid "Error with the request: %s"
msgstr "Gabim me kërkesën: %s"

#: ckanext/archiver/tasks.py:977
#, python-format
msgid "Server returned HTTP error status: %s %s"
msgstr "Serveri u kthye statusin e gabimit HTTP: %s %s"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:38
msgid "Validation"
msgstr "Validimi"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:44
msgid "Link is broken"
msgstr "Lidhja është e prishur"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:49
msgid "This is a one-off failure"
msgstr "Ky është një dështim i njëhershëm"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:51
#, python-format
msgid ""
"This resource has failed %(failure_count)s times in a row since it first "
"failed: %(first_failure)s"
msgstr ""
"Ky burim ka dështuar %(failure_count)s herë me radhë që nga dështimi i parë: "
"%(first_failure)s"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:54
#, python-format
msgid "This resource was last ok: %(last_success)s"
msgstr "Ky burim ishte i fundit në rregull: %(last_success)s"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:56
#, python-format
msgid ""
"We do not have a past record of it working since the first check: %(created)s"
msgstr ""
"Ne nuk kemi një rekord të mëparshëm të punës që nga kontrolli i parë: "
"%(created)s"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:61
msgid "Link check is not conclusive"
msgstr "Kontrolli i lidhjes nuk është përfundimtar"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:68
msgid "Link is ok"
msgstr "Lidhja është në rregull"

#: ckanext/archiver/templates/archiver/is_resource_broken.html:80
#, python-format
msgid "Link checked: %(updated)s"
msgstr "Lidhja është e kontrolluar: %(updated)s"

